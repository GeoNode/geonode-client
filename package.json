{
  "name": "geonode-viewer",
  "version": "0.0.0",
  "scripts": {
    "createdir": "node createbuild.js",
    "start:debug": "node debug-server.js",
    "start": "node_modules/.bin/npm-run-all createdir start:debug",
    "build:js": "node_modules/.bin/browserify -g [envify --NODE_ENV production] -p [minifyify --no-map --exclude node_modules/openlayers/dist/ol.js] app.jsx -o build/app.js",
    "build:lib": "node_modules/.bin/browserify -g [envify --NODE_ENV production] -p [minifyify --no-map --exclude node_modules/openlayers/dist/ol.js] geonode.jsx -o build/geonode.js",
    "build:lib:debug": "node_modules/.bin/browserify -g [envify --NODE_ENV development] geonode.jsx -o build/geonode-debug.js",
    "build": "node_modules/.bin/npm-run-all createdir build:js",
    "createzip": "node createpackage.js",
    "package": "node_modules/.bin/npm-run-all build createzip",
    "test:watch": "mochify --require intl --recursive --watch tests/**/*.test.js",
    "test:phantom": "mochify --require intl --recursive tests/**/*.test.js",
    "test:wd": "mochify --require intl --recursive --wd tests/**/*.test.js",
    "test:cover": "mochify --cover tests/**/*.test.js",
    "test": "npm run test:phantom"
  },
  "dependencies": {
    "boundless-sdk": "^0.9.0",
    "material-ui": "0.15.4",
    "ol3-cesium": "3.9.4",
    "openlayers": "~3.17.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-intl": "2.0.0-rc-1",
    "react-tap-event-plugin": "^1.0.0"
  },
  "devDependencies": {
    "archiver": "0.21.0",
    "babel-plugin-react-intl": "1.0.0-beta-5",
    "babelify": "^6.0.2",
    "boundless-sdk-tools": "^0.9.0",
    "browserify": "^11.2.0",
    "chai": "^3.5.0",
    "cssify": "^0.8.0",
    "envify": "^3.4.0",
    "fs-extra": "0.26.2",
    "intl": "^1.2.5",
    "minifyify": "^7.1.0",
    "mocha": "^3.1.2",
    "mochify": "^2.18.1",
    "npm-run-all": "1.3.1",
    "phantomjs-polyfill-object-assign": "0.0.2",
    "raf": "^3.3.0",
    "react-addons-test-utils": "^15.3.2",
    "react-test-renderer": "^15.3.2",
    "react-tools": "0.12.0",
    "readline": "1.3.0",
    "replacestream": "4.0.0",
    "through": "^2.3.8",
    "watchify": "^3.2.0"
  },
  "browserify": {
    "transform": [
      "cssify",
      "babelify"
    ]
  }
}
